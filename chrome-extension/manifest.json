{
    "name": "RepoWizard",
    "description": "Extension to help generate documentation for Github repos",
    "version": "1.0.0",
    "manifest_version": 3,
    "host_permissions": [
      "*://*/*"
    ],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.component.js"]
    }
  ],
    "action": {
      "default_popup": "./app.component.html"
    }
  }